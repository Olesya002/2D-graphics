phi = -3*pi:0.01:3*pi;
n = 2; m = 2; 
x = cos(n*phi).*(cos(phi)).^m;
y = sin(n*phi).*(sin(phi)).^m;
plot(x,y);
xlabel('x')
ylabel('y')
grid on


x = -10:.1:10;
y = 1./x-1./(x-1)+1./(x-2);
plot(x,y);
grid on

phi = -3*pi:.01:3*pi;
a = 2;
b = 1;
c = 1;
x = a.*(b.*cos(phi)+phi.*sin(phi));
y = a.*(sin(phi)-c.*phi.*cos(phi));
plot(x,y);
grid on
